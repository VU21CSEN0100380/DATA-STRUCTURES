#include<stdio.h>
int main()
{
    int ar[5]={10,20,30,40,50};
    int i, key,flag=0;
    printf("enter an element:");
    scanf("%d",&key);
    for(i=0;i<5;i++)
    {
      if(key==ar[i])
      {
          flag=1;
          break;
      }
    }
    if(flag==1)
    printf("key is  found");
    else
    printf("key is not found");
}
